{
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs18.x"
      }
    }
  }